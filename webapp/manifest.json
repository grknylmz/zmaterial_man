{
  "_version": "1.8.0",
  "sap.app": {
    "id": "gqsystema.zmaterial_man",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.38.11"
    },
    "dataSources": {
      "materialService": {
        "uri": "/sap/opu/odata/sap/zmaterial_master_srv",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_hcb", "sap_bluecrystal"]
  },

  "sap.ui5": {
    "rootView": {
      "viewName": "gqsystema.zmaterial_man.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "gqsystema.zmaterial_man.i18n.i18n"
        }
      },
      "materialModel": {
        "type": "sap.ui.model.odata.v2.ODataModel",
        "settings": {
          "defaultOperationMode": "Server",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Request"
        },
        "dataSource": "materialService",
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ],
      "js": [
        {
          "uri": "libs/quagga.min.js",
          "async": true
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "gqsystema.zmaterial_man.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false,
        "bypassed": {
          "target": ["notFound"]
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "materialDetail",
          "target": ["materialDetail"]
        }
      ],
      "targets": {
        "materialDetail": {
          "viewName": "MaterialDetail",
          "viewId": "MaterialDetail",
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewLevel": 1,
          "routerClass": "sap.m.routing.Router",
          "viewPath": "gqsystema.zmaterial_man.view",
          "controlId": "app",
          "async": true
        },
        "notFound": {
          "viewType": "XML",
          "viewName": "NotFound"
        }
      }
    }
  }
}
